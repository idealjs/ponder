{
  "name": "@idealjs/ponder-service-trpc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:key": "openssl req -x509 -newkey rsa:2048 -nodes -sha256 -subj '/CN=localhost' -addext 'subjectAltName = DNS.1:localhost' -keyout server.key -out server.crt",
    "dev": "nodemon",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^16.6.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@idealjs/ponder-shared-node": "^0.1.0",
    "@trpc/server": "^10.9.0",
    "fastify": "^4.11.0",
    "zod": "^3.20.2"
  }
}
