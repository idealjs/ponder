  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.5.0",
    "fastify-type-provider-zod": "^1.1.9",
    "qs": "^6.11.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@prisma/client": "^4.14.1",
    "@swc/core": "^1.3.24",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^20.2.1",
    "@types/qs": "^6.9.7",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  }
}
